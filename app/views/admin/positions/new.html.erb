<% provide(:title, 'New position') %>
<h1>New position</h1>

<div class="row">
  <div class="span6 offset3">
    <%= form_for [:admin, @position] do |f| %>
      <%= render 'fields', f: f %>
      <%= f.submit "Create new position", class: "btn btn-large btn-primary" %>
    <% end %>
  </div>
</div>